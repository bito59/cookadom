= form_tag session_path(:user), html: { autocomplete: "off" } do
  = text_field_tag 'user[email]', nil, required: 'true', placeholder: 'email'
  = password_field_tag 'user[password]', nil, placeholder: 'password'
  %button Sign in
  %br
  = link_to "Forgot your password?", new_password_path(:user)

